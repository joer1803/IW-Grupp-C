@model HampesFrilansare.Models.Customer

@{
    ViewBag.Title = "Radera företag";
}

<h4>Ta bort</h4>

<h5>Är du säker på att du vill ta bort detta företag?</h5>
<div>
    <h4>Företag</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Företagsnamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.businessname)
        </dd>

        <dt>
            Förnamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.firstname)
        </dd>

        <dt>
            Efternamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.lastname)
        </dd>

        <dt>
            Telefon
        </dt>

        <dd>
            @Html.DisplayFor(model => model.phonenumber)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            Adress
        </dt>

        <dd>
            @Html.DisplayFor(model => model.address)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Ta bort" class="btn btn-default" /> |
            @Html.ActionLink("Tillbaka till lista", "Index")
        </div>
    }
</div>

@model IEnumerable<HampesFrilansare.Models.Freelancer>

@{
    ViewBag.Title = "Index";
}

<h2>Frilansare</h2>

<p>
    @Html.ActionLink("Skapa ny användare", "Create")
</p>
<table class="table-dark table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lastname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phonenumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dateofbirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.birthplace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nationality)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phonenumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateofbirth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.birthplace)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nationality)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.freelancerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.freelancerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.freelancerID })
        </td>
    </tr>
}

</table>

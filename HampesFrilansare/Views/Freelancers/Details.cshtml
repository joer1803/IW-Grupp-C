@model HampesFrilansare.Models.Freelancer

@{
    ViewBag.Title = "Kontaktuppgifter";
}

<h4>Kontaktuppgifter</h4>

<div>
    <h4>Frilansare</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Förnamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.firstname)
        </dd>

        <dt>
            Efternamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.lastname)
        </dd>

        <dt>
            Telefon
        </dt>

        <dd>
            @Html.DisplayFor(model => model.phonenumber)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            Adress
        </dt>

        <dd>
            @Html.DisplayFor(model => model.address)
        </dd>

        <dt>
            Födelsedatum
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dateofbirth)
        </dd>

        <dt>
            Födelseort
        </dt>

        <dd>
            @Html.DisplayFor(model => model.birthplace)
        </dd>

        <dt>
            Nationalitet
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nationality)
        </dd>
        <dd>
            @Html.DisplayFor(model => model.Resume.ToString())
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Ändra", "Edit", new { id = Model.freelancerID }) |
    @Html.ActionLink("Tillbaka till lista", "Index")
</p>

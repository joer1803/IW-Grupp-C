@model HampesFrilansare.Models.Freelancer

@{
    ViewBag.Title = "Ta bort";
}

<h2>Ta bort</h2>

<h3>Är du säker på att du vill ta bort den här personen?</h3>
<div>
    <h4>Frilansare</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Förnamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.firstname)
        </dd>

        <dt>
            Efternamn
        </dt>

        <dd>
            @Html.DisplayFor(model => model.lastname)
        </dd>

        <dt>
            Telefon
        </dt>

        <dd>
            @Html.DisplayFor(model => model.phonenumber)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            Adress
        </dt>

        <dd>
            @Html.DisplayFor(model => model.address)
        </dd>

        <dt>
            Födelsedatum
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dateofbirth)
        </dd>

        <dt>
            Födelseort
        </dt>

        <dd>
            @Html.DisplayFor(model => model.birthplace)
        </dd>

        <dt>
           Nationalitet
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nationality)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Ta bort" class="btn btn-default" /> |
            @Html.ActionLink("Tillbaka till lista", "Index")
        </div>
    }
</div>

@model HampesFrilansare.Models.Customer
@{
    ViewBag.Title = "SignupCustomer";
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm())
{

    @Html.ValidationSummary(true, "", new { @class = "invalid-feedback" })
<form>
    <div class="form-group">
        @Html.LabelFor(model => model.email, labelText: "Email")
        @Html.EditorFor(model => model.email, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    @*@Html.LabelFor(model => model., labelText: "Lösenord")
        @Html.EditorFor(model => model., new { htmlAttributes = new { @class = "form-control" } })*@
    <div class="form-group">
        <label for="password">Lösenord</label>
            <input class="form-control" type="password"/>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.businessname, labelText: "Företagsnamn")
        @Html.EditorFor(model => model.businessname, new { htmlAttributes = new { @class = "form-control" } })
    </div>
</form>
}



@model HampesFrilansare.Models.Driverslicence
@{
    ViewBag.Title = "Körkort och språk";
    Layout = "~/Views/Shared/_NavbarFreelancer.cshtml";
}

<h4 class="generalformhead4">Körkort</h4>
@using (Html.BeginForm("EditDriverslicence", "Resumes"))
{
    @Html.AntiForgeryToken()

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(model => model.type, htmlAttributes: new { @class = "control-label" }, labelText: "Körkortstyp")
            @Html.EditorFor(model => model.type, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.type, "", new { @class = "text-danger" })
    </div>
    <div class="form-group">
            <input type="submit" value="Spara" class="btn btn-secondary" />
    </div>
    <div>
        @Html.ActionLink("Tillbaka till min profil", "FreelancerProfile", "Freelancers")
    </div>
}

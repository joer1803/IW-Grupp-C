@model HampesFrilansare.Models.Resume

@{
    ViewBag.Title = "Radera CV";
}

<h4>Ta bort</h4>

<h6>Är du säker på att du vill radera detta CV?</h6>
<div>
    <h4>CV</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Profil
        </dt>

        <dd>
            @Html.DisplayFor(model => model.profile)
        </dd>

        <dt>
           Kärnförmågor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.coreability)
        </dd>

        <dt>
           Körkort
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Driverslicence.type)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Ta bort" class="btn btn-default" /> |
            @Html.ActionLink("Tillbaka till lista", "Index")
        </div>
    }
</div>
